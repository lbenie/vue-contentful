{
  "name": "vue-contentful-hook",
  "version": "1.1.0",
  "description": "A vue hook to fetch contentful data using GraphQL query",
  "repository": {
    "type": "git",
    "url": "https://github.com/lbenie/vue-contentful-hook.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "graphql",
    "contentful",
    "hook"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/vue-contentful-hook.umd.js",
  "module": "./dist/vue-contentful-hook.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-contentful-hook.es.js",
      "require": "./dist/vue-contentful-hook.umd.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest,stryker 'jest --coverage' 'stryker run'",
    "test:mutation": "stryker run",
    "format": "eslint 'lib/**/*.ts' --fix",
    "build": "vite build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@stryker-mutator/core": "5.6.1",
    "@stryker-mutator/jest-runner": "5.6.1",
    "@types/jest": "27.4.0",
    "@types/semantic-release": "17.2.3",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "all-contributors-cli": "6.20.0",
    "concurrently": "7.0.0",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.0",
    "jest-chain": "1.1.5",
    "jest-extended": "2.0.0",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "ts-jest": "27.1.3",
    "ts-jest-mock": "1.1.31",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vue": "3.2.29"
  },
  "peerDependencies": {
    "vue": "^3.2.29"
  }
}
