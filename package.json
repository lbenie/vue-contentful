{
  "name": "vue-contentful-hook",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "version": "1.4.0",
  "description": "A vue hook to fetch contentful data using GraphQL query",
  "repository": {
    "type": "git",
    "url": "https://github.com/lbenie/vue-contentful-hook.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "graphql",
    "contentful",
    "hook"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/vue-contentful-hook.umd.cjs",
  "module": "./dist/vue-contentful-hook.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-contentful-hook.js",
      "require": "./dist/vue-contentful-hook.umd.cjs"
    }
  },
  "types": "dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "concurrently -n jest,stryker 'jest --coverage' 'stryker run'",
    "test:mutation": "stryker run",
    "format": "eslint 'lib/**/*.ts' --fix",
    "build": "vite build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.5.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@stryker-mutator/core": "6.2.3",
    "@stryker-mutator/jest-runner": "6.2.3",
    "@types/jest": "29.1.2",
    "@types/semantic-release": "17.2.4",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "all-contributors-cli": "6.24.0",
    "axios": "0.27.2",
    "concurrently": "7.4.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.1.2",
    "jest-chain": "1.1.6",
    "jest-extended": "3.1.0",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "ts-jest": "29.0.3",
    "ts-jest-mock": "1.1.33",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "vite": "3.1.7",
    "vue": "3.2.40"
  },
  "peerDependencies": {
    "axios": "^0.26.1 || ^0.27.0",
    "vue": "^3.2.29"
  }
}
